{
  "manifest_version": 2,

  "name": "Cyber Buddy",
  "description": "This extension blocks you from online harrasement, and tracks online individuals who are mean and bullys!",
  "version": "0.1",
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
        {
            "matches": [

                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}
